.user-info{
  height:150rpx;
  width:100%;
  border-bottom: rgba(94, 182, 249,0.5) solid;
  border-left: none;
  border-right: none;
  display:flex;
  align-items: center;
}
.user-info image{
  margin-left: 20rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}
.user-info text{
  margin-left: 20rpx;
}
.user-info button{
  margin-top: 90%;
  width: 80%;
  height: 50%;
  text-align: center;
  border-radius: 98rpx;
  font-size: 120%;
  color: rgb(249, 94, 97,0.7);
  background-color: rgba(155, 231, 249, 0.453);
  
}
.nickName{
  color: red;
}
.user-list{
  margin-top: 20rpx;
}
.user-list-item{
  height: 50rpx;
  border-bottom: 4px rgba(155, 231, 249, 0.453) dashed;
  padding: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color:#1c3c62;
}

.showout{
  margin-top: 6%;
  border-top: 2px rgba(4, 198, 242, 0.447) dashed;
}
.showtable{

  justify-content: left;
  margin-top:170rpx;
  margin-left: 10%;
  width: 80%;
  margin-top: 20rpx;
  background-color:rgba(33, 191, 249, 0.433);
  color: rgb(255, 255, 255);
  border-radius: 28rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.lable{
  padding: 5%;
}

